                    "queryType": "sql",
                    "rawSql": "SELECT\r\n  user_name as User,\r\n  sum(JSONExtractUInt(payload, 'working_time')) / 1000 / 3600 as \"Working time(h)\",\r\n  count() as Activity,\r\n  countIf(scope IN ('draw:object')) as \"Activity(draw object)\",\r\n  countIf(scope NOT IN ('draw:object')) as \"Activity(else)\"\r\nFROM events\r\nWHERE JSONHas(payload, 'working_time')\r\n  AND $__timeFilter(timestamp)\r\n  AND user_name IN (${users})\r\n  AND (-1 IN (${projects}) OR project_id IN (${projects}))\r\n  AND (-1 IN (${tasks}) OR task_id IN (${tasks}))\r\n  AND (-1 IN (${jobs}) OR job_id IN (${jobs}))  \r\nGROUP BY user_id, user_name",
                    "refId": "A"
                }
            ],
            "title": "Working time & Activity by user",
